{
  "name": "noflo-objects",
  "description": "Object Utilities for NoFlo",
  "keywords": [
    "noflo",
    "objects",
    "utilities"
  ],
  "author": "Kenneth Kan <kenhkan@gmail.com>",
  "version": "0.0.7",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/kenhkan/noflo-objects/blob/master/LICENSE.md"
    }
  ],
  "engines": {
    "node": ">=0.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kenhkan/noflo-objects"
  },
  "dependencies": {
    "noflo": "https://github.com/kenhkan/noflo/tarball/master",
    "underscore": "1.4.x",
    "underscore.string": "2.3.x",
    "owl-deepcopy": "0.0.x",
    "noflo-lift": "0.0.x"
  },
  "devDependencies": {
    "coffeelint": "*",
    "coffee-script": "1.6.x",
    "noflo-test": "https://github.com/kenhkan/noflo-test/tarball/master"
  },
  "noflo": {
    "components": {
      "Extend": "./components/Extend.coffee",
      "MergeObjects": "./components/MergeObjects.coffee",
      "SplitObject": "./components/SplitObject.coffee",
      "ReplaceKey": "./components/ReplaceKey.coffee",
      "Keys": "./components/Keys.coffee",
      "Values": "./components/Values.coffee",
      "Join": "./components/Join.coffee",
      "FilterProperty": "./components/FilterProperty.coffee",
      "ExtractProperty": "./components/ExtractProperty.coffee",
      "InsertProperty": "./components/InsertProperty.coffee"
    },
    "graphs": {
      "GroupValueByAnother": "./graphs/GroupValueByAnother.fbp"
    }
  },
  "scripts": {
    "pretest": "./node_modules/.bin/coffeelint -r components",
    "test": "./node_modules/.bin/noflo-test --spec test/*.coffee"
  }
}
